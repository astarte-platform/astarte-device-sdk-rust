{
  "interface_name": "org.astarte-platform.rust.examples.individual-datastream.VolatileDeviceObject",
  "version_major": 0,
  "version_minor": 1,
  "type": "datastream",
  "aggregation": "object",
  "ownership": "device",
  "mappings": [
    {
      "endpoint": "/endpoint/longinteger",
      "type": "longinteger",
      "reliability": "guaranteed",
      "retention": "volatile",
      "expiry": 30
    },
    {
      "endpoint": "/endpoint/boolean",
      "type": "boolean",
      "reliability": "guaranteed",
      "retention": "volatile",
      "expiry": 30
    }
  ]
}
